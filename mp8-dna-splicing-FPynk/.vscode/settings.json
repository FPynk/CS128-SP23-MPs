{
    "clang-format.fallbackStyle": "Google",
    "editor.formatOnSave": true,
    "clang-tidy.lintOnSave": true,
    "clang-tidy.blacklist": [],
    "clang-tidy.compilerArgs": [
        "-std=c++20"
    ],
    "clang-tidy.compilerArgsBefore": [
        "-Iincludes",
        "-Wunused-variable",
        "-Wuninitialized",
        "-Wunnused"
    ],
    "C_Cpp.clang_format_sortIncludes": false,
    "C_Cpp.formatting": "Disabled",
    "C_Cpp.inactiveRegionOpacity": null,
    "files.exclude": {
        "**/.*": true
    },
    "files.associations": {
        "ostream": "cpp",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "exception": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "numbers": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "system_error": "cpp",
        "type_traits": "cpp",
        "typeinfo": "cpp"
    }
}